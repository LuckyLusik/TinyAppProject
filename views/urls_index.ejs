  <title>TinyApp</title>
</head>
<body>
  <div class="center">
<header>
    <% include ./partials/_header.ejs %>
</header>

    <ul>
    <% for (shortname in urls) { %>
<form method="POST" action="/urls/<%= shortname %>/delete">
