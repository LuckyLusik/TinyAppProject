<% if (!userObj) { %>
    <p class="marg">Please, <a href='/register'>Register</a>
    or <a href='/login'>Login</a> first</p>
<% } else { %>
      <p style="font-size:25px;"><strong><%= userObj.email %></strong></p>
      <form method="POST" action="/logout">
      <input type="submit" value="Logout" class="button">
      </form>
<% } %>
